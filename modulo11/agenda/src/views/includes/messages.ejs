<% if(errors.length > 0) { %>
  <div class="row">
    <div class="col my-3">
      <% errors.forEach(function(error) { %>
        <div class="alert alert-danger"><%= error %></div>
      <% }); %>
    </div>
  </div>
<% } %>

<% if (success && success.length > 0) { %>
  <div class="row">
    <div class="col my-3">
      <% success.forEach(function(msg) { %>
        <div class="alert alert-success"><%= msg %></div>
      <% }); %>
    </div>
  </div>
<% } %>